<templateSet group="Swift">
  <template name="newfunc" value="func $NAME$($PARAMETERS$) $RETURN_CLAUSE${&#10;    $END$&#10;}" description="Function statement" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="&quot;name&quot;" defaultValue="" alwaysStopAt="true" />
    <variable name="PARAMETERS" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="RETURN_CLAUSE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="swift.lang.declaration.context" value="true" />
      <option name="swift.lang.statement.context" value="true" />
    </context>
  </template>
  <template name="sout" value="print(&quot;$statement$&quot;)" description="print(&quot;$statement$&quot;)" toReformat="true" toShortenFQNames="true">
    <variable name="statement" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="swift.lang.declaration.context" value="true" />
      <option name="swift.lang.statement.context" value="true" />
    </context>
  </template>
  <template name="sb" value="{$statemtn$}" description="{$statemtn$}" toReformat="true" toShortenFQNames="true">
    <variable name="statemtn" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="swift.lang.declaration.context" value="true" />
      <option name="swift.lang.statement.context" value="true" />
    </context>
  </template>
</templateSet>